{
	"name": "grapes",
	"description": "",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"preview": true,
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "grapes.showPanel",
				"title": "Toggle Grapes panel"
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "resourceLangId == html",
					"command": "grapes.showPanel",
					"alt": "grapes.showPanel",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc  -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -w -p ./",
		"build": "webpack"
	},
	"dependencies": {},
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"@types/node": "^10.5.2",
		"@types/vscode": "^1.32.0",
		"babel-loader": "^8.0.6",
		"copy-webpack-plugin": "^5.0.3",
		"css-loader": "^2.1.1",
		"grapesjs": "^0.14.62",
		"style-loader": "^0.23.1",
		"tslint": "^5.16.0",
		"typescript": "^3.4.5",
		"webpack": "^4.32.2",
		"webpack-cli": "^3.3.2"
	}
}
